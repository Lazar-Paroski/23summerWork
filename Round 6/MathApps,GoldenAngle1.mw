<Startup-Code startupcode="\012#Macro Stuff\012macro(SP = DocumentTools:-SetProperty);\012macro(GP = DocumentTools:-GetProperty);\012\012########################## Start of Module ####################\012GoldenAngle := module()\012uses plots,plottools,ArrayTools,Degrees,DocumentTools;\012export sliderStuff,startingVals,updateAngle, mathStuff, changeInput,generatePts,animateSlider; \012\012#Stating local vars\012#Won't Change.\012local middleP,firstP,n;\012#Will change.\012local angleChange,angle1,newRadius,PA,DA,MA,SA,UC,GA,rotVar,maxVal,xNew,yNew;\012#From rFind\012local xLine,yLine,litAngle,setD,originD,inerAngle,goodAngle,goodD;\012\012#################### Checkbox to animate the slider\012animateSlider := proc()\012\012\009if GP('CheckBox1','value') = &quot;true&quot; then\012\009\009#resetting slider and plot\012\009\009SP('Slider0','value',0);\012\009\009local update_plot := display(convert(GA[1..2],list),scaling=constrained,axes=none);\012\009\009SP(&quot;Plot0&quot;, 'value', update_plot);\012\009\009#Animating plot\012\009\009local t;\012\009\009SP(&quot;Plot0&quot;, 'value', animate('plots:-display, [convert(GA[1..floor(t)],list),scaling=constrained,axes=none]', t=2..100,frames=100,paraminfo=false),'refresh'=true);\012\009\009#Starting Animation\012\009\009SP(&quot;Plot0&quot;, 'play', true,'refresh'=true);\012\009\009SP(&quot;Plot0&quot;, 'continuous', true,'refresh'=true);\012\009else\012\009\009#resetting slider and plot\012\009\009SP('Slider0','value',0);\012\009\009local update_plot := display(convert(GA[1..2],list),scaling=constrained,axes=none);\012\009\009SP(&quot;Plot0&quot;, 'value', update_plot);\009\012\009end if;\012\012end proc;\012#################### Button that updates angle.\012updateAngle := proc()\012\009local angleVal;\012\009angleChange := parse(GP('TextArea0','value'));\012\012\009if type(angleChange, numeric) then\012\009\009#resetting slider and plot\012\009\009SP('Slider0','value',0);\012\009\009#Update Slider\012\009\009SP('Slider0', 'upper', 100);\012\009\009local update_plot := display(convert(GA[1..2],list),scaling=constrained,axes=none);\012\009\009SP(&quot;Plot0&quot;, 'value', update_plot);\012\009\009#Adjusting angle if check box is true\012\009\009if GP('CheckBox0','value') = &quot;true&quot; then\012\009\009\009angleChange := (angleChange-floor(angleChange))*360;\012\009\009end if;\012\009\009#Making the array\012\009\009dummyval := mathStuff(100);\012\009\009SP('Label0','Caption',&quot;&quot;);\012\009else\012\009\009SP('Label0','Caption',&quot;Please enter a number!&quot;);\012\009end if;\012end proc;\012#################### Button that generates points.\012generatePts := proc()\012\009#Adjusting animate box\012\009if GP('CheckBox1','value') = &quot;true&quot; then\012\009\009SP('CheckBox1','value',&quot;false&quot;);\012\009end if;\012\009#Resetting values to default\012\009resetVals();\012\009#Getting input\012\009local pointsToPlot := parse(GP('TextArea1','value'));\012\009if type(pointsToPlot, posint) and pointsToPlot &gt; 99 then\012\009\009dummyval := mathStuff(pointsToPlot);\012\009\009SP('Label1','Caption',&quot;&quot;);\012\009\009#Update Slider\012\009\009SP('Slider0', 'upper', pointsToPlot);\012\009\009#plot the points\012\009\009local update_plot := display(convert(GA[1..pointsToPlot+2],list),scaling=constrained,axes=none);\012\009\009SP(&quot;Plot0&quot;, 'value', update_plot);\012\009else\012\009\009SP('Label1','Caption',&quot;Please enter a positive integer greater than 99!&quot;)\012\009end if;\012\009\012end proc;\012\012################### Check box to enter in rot.\012changeInput := proc()\012\009rotVar:=rotVar+1;\012end proc;\012#################### Slider Proc to Update Graph.\012sliderStuff := proc()\012\009\012\009#Adjusting animate box\012\009if GP('CheckBox1','value') = &quot;true&quot; then\012\009\009SP('CheckBox1','value',&quot;false&quot;);\012\009end if;\012\009\012\009local sliderVal;\012\009sliderVal := floor(GP('Slider0', 'value'));\012\012\009if sliderVal = sliderCompare then\012\009\009#do nothing\012\009elif sliderVal &lt;&gt; UC then\012\009\009local update_plot := display(convert(GA[1..sliderVal+2],list),scaling=constrained,axes=none);\012\009\009SP(&quot;Plot0&quot;, 'value', update_plot);\012\009\009sliderCompare := sliderVal;\012\009end if;\012\009\012end proc; #sliderStuff\012\012####################### Proc that does all the math. (i.e. makes graphList to 302 points. 300 points + 2 that are alredy made)\012mathStuff := proc(steps)\012\009local c:=0;\012\009#Resetting values to default\012\009UC:=1:\012\009angle1:=0:\012\009newRadius:=2:\012\009#Adjusting input if check box is ticked\012\009\012\009while c &lt; steps do\012\009\009c:=c+1;\012\009\009\012\009\009#Updating universal counter\012\009\009UC := UC+1;\012\009\009\012\009\009#Updating angle\012\009\009angleChange:= evalf(angleChange);\012\009\009angle1 := angle1 + angleChange;\012\009\009angle1 := simpleAngle(angle1);\012\009\009\009\012\009\009#Getting x and y coords for next potential point.\012\009\009xNew,yNew := placeC(newRadius,angle1);\012\009\009\012\009\009#Updating Distance array\012\009\009for local row from 1 to UC do\012\009\009\009DA[row]:= dFind(PA[row,1],PA[row,2],xNew,yNew);\012\009\009end do;\012\009\009\012\009\009#Updating Mega Array\012\009\009for local row from 1 to UC do\012\009\009\009MA[row,1] := DA[row];\012\009\009\009MA[row,2] := PA[row,1];\012\009\009\009MA[row,3] := PA[row,2];\012\009\009end do;\012\009\009\009\012\009\009#Sorting the array and storing in sorted array\012\009\009local subMA := MA[1..UC];\012\009\009subMA:=SortBy(subMA, 'column', 1);\012\009\009SA[1..UC]:=subMA;\012\009\009\012\009\009#Updating radius if the distance is too close\012\009\009local p:=0;\012\009\009#for element in distanceList do\012\009\009\009\012\009\009for local row from 1 to UC do\012\009\009\009\009\012\009\009\009if DA[row] = 0 then\012\009\009\009\009newRadius := newRadius + 2;\012\009\009\009\009xNew,yNew := placeC(newRadius,angle1);\012\009\009\009\009break;\012\009\009\009elif DA[row] &lt; 1.99 then\012\009\009\009\009newRadius := rFind(SA[1,2],SA[1,3],angle1); \012\009\009\009\009xNew,yNew := placeC(newRadius,angle1);\012\009\009\009\009break;\012\009\009\009elif DA[row] &gt; 2.1 then\012\009\009\009\009p := p+1;\012\009\009\009else\012\009\009\009\009next;\012\009\009\009end if;\012\009\009end do;\012\009\009\009\012\009\009\009#If no point touch it is fixed\012\009\009if p = UC then \012\009\009\009newRadius := rFind(SA[1,2],SA[1,3],angle1); \012\009\009\009xNew,yNew := placeC(newRadius,angle1);\012\009\009end if;\012\009\009\012\009\009#Adding new point to the point array\012\009\009PA[UC+1,1] := xNew;\012\009\009PA[UC+1,2] := yNew;\012\012\009\009#Setting random color\012\009    \009local myPalette:= ColorTools:-GetPalette(&quot;Plasma&quot;);\012\009    \009# Getting random number for when I want to pick random color\012\009    \009randomize();\012\009    \009local r1:= rand(1..256)();\012\009    \009#Picking Random color\012\009    \009local c1:=myPalette[r1];\012\009\009\012\009\009#Appending the new disk to the end of the array of graphs\012\009\009local newG := disk([xNew,yNew],1,color=c1);\012\009\009GA[UC+1]:=newG;\012\009end do;\012\009return 0;\009\012end proc; #mathStuff\012\012######################## Point maker proc.\012placeC := proc(r,a)\012\009xNew := r*evalf[9](cosd(a));\012\009yNew := r*evalf[9](sind(a));\012\009return xNew,yNew\012end proc; #placeC\012######################## Radius finder proc. #########h\012\012rFind := proc(xClosest,yClosest,angle1)\009\012\009setD := 2;\012\009xLine,yLine := angleToVector(angle1);\012\009litAngle := vectorAngle(xLine,yLine,xClosest,yClosest);\012\009\012\009if litAngle = 0 then\012\009\009#special condition for litAngle is zero\012\009\009goodD := dFind(0,0,xClosest,yClosest)+2;\012\009else\012\009\009originD := dFind(0,0,xClosest,yClosest);\012\009\009if Im(solve((setD/originD)=(evalf(sind(litAngle))/evalf(sind(p))),p)) &lt;&gt; 0 then\012\009\009\009#Special condition for inerAngle Imaginary\012\009\009\009goodD := dFind(0,0,xClosest,yClosest)+2;\012\009\009else\012\009\009\009inerAngle := solve((setD/originD)=(evalf(sind(litAngle))/evalf(sind(p))),p);\012\009\009end if;\012\009\009\012\009\009goodAngle := 180 - litAngle - inerAngle;\012\009\009\012\009\009if Im(solve((setD/p)=evalf(sind(litAngle))/evalf(sind(goodAngle)),p)) &lt;&gt; 0 then\012\009\009\009#Special condtion for goodD imaginary\012\009\009\009goodD := dFind(0,0,xClosest,yClosest)+2\012\009\009else\012\009\009\009goodD := solve((setD/p)=evalf(sind(litAngle))/evalf(sind(goodAngle)),p);\012\009\009end if;\012\009end if;\012\009return goodD;\012end proc; #rFind\012\012######################## Angle to vector proc.\012angleToVector := proc(angle1)\012\009local x,y;\012\009x := evalf(cosd(angle1));\012\009y := evalf(sind(angle1));\012\009return x,y;\012end proc:\012\012####################### Angle between two vectors proc.\012vectorAngle := proc(xa,ya,xb,yb)\012\009local magA, magB, dotProd,theta;\012\009magA := sqrt((xa)^(2)+(ya)^(2));\012\009magB := sqrt((xb)^(2)+(yb)^(2));\012\009dotProd := xa*xb + ya*yb;\012\009if magA = 0 then\012\009\009theta := arctand(yb/xb)\012\009elif magB = 0 then\012\009\009theta := arctand(ya/xa)\012\009else\012\009\009theta := solve(evalf(cosd(p))=(dotProd/(magA*magB)),p); \012\009end if;\012\009return theta;\012end proc:\012\012##################### distance between two points finder.\012dFind := proc(x0,y0,x1,y1)\012\009local dFound := sqrt(((x1-x0)^(2))+((y1-y0)^(2)));\012\009return dFound\012end proc:\012\012##################### Simple angle proc.\012simpleAngle := proc(a)\012\009local numOfTurn;\012\009if a/360 &gt;= 1 then\012\009\009numOfTurn := floor(a/360);\012\009\009return a - 360*numOfTurn; \012\009else\012\009\009return a;\012\009end if;\012end proc:\012\012######################## Initalizing Values Proc.\012startingVals:= proc()\012\012\009#Length of all arrays (i.e. max amount of points I am letting the user plot)\012\009n:=10000:\012\009\012\009#Making graph Array\012\009middleP:= disk([0,0],1,color=purple):\012\009firstP := disk([2,0],1,color=yellow):\012\009GA:= Array(1..n):\012\009GA[1]:= middleP:\012\009GA[2]:= firstP:\012\009\012\009#Making point array\012\009PA:= Array(1..n,1..2):\012\009PA[1,1]:= 0:\012\009PA[1,2]:= 0:\012\009PA[2,1]:= 2:\012\009PA[2,2]:=0:\012\012\009#Making distance, mega, and sorted array. \012\009#The mega arrray will be a combonation of the point and distance arrays.\012\009#The sorted array will be a sorted version of the mega array. It will be sorted by distance.\012\009DA:= Array(1..n):\012\009MA:= Array(1..n,1..3):\012\009SA:= Array(1..n,1..3):\012\012\009#Update Slider\012\009SP('Slider0', 'upper', 100);\012\009SP('Slider0','value',0);\012\009#Universal counter, that will keep track of the number of plotted points.\012\009#DOES NOT INCLUDE THE FIRST POINTS\012\009UC:=1:\012\012\009#How much the angle will change with each rotation\012\009angleChange:=123:\012\012\009#Current Angle\012\009angle1:=0:\012\012\009#The radius that the point is going to be placed at\012\009newRadius:=2:\012\012\009#Keeps track of changing the input method so it is known what to change the input method to \012\009#Might not be needed anymore although I could still use a button\012\009rotVar:=0:\012\012\009#Value used to see if the value of the slider has changed\012\009sliderCompare := 0:\012\012\009#Calling math stuff to make the inital list \012\009dummyval := mathStuff(100);\012\012\009#Plotting inital points\012\009local update_plot := display(convert(GA[1..2],list),scaling=constrained,axes=none);\012\009SP(&quot;Plot0&quot;, 'value', update_plot);\012\009\012end proc; #startingVals \012\012############################## resets some of the values.\012resetVals := proc()\012\009#Length of all arrays (i.e. max amount of points I am letting the user plot)\012\009n:=10000:\012\009\012\009#Making graph Array\012\009middleP:= disk([0,0],1,color=purple):\012\009firstP := disk([2,0],1,color=yellow):\012\009GA:= Array(1..n):\012\009GA[1]:= middleP:\012\009GA[2]:= firstP:\012\012\009#Universal counter, that will keep track of the number of plotted points.\012\009#DOES NOT INCLUDE THE FIRST POINTS\012\009UC:=1:\012\012\009#Current Angle\012\009angle1:=0:\012\012\009#The radius that the point is going to be placed at\012\009newRadius:=2:\012end proc;\012\012end module; #GoldenAngel  ######################## End of Module ########################\012\012\012#Calling starting vals to make &quot;state&quot; variables\012GoldenAngle:-startingVals();\012#Macro stuff\012macro(SP = SP, GP = GP);"/>
