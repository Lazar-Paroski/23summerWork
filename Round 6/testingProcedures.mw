</Group><CodeEditor-ExecGroup view="presentation" labelreference="L30" drawlabel="true" applyint="true" applyrational="true" display="code"><EC-CodeEditor id="CodeEditRegion0" expanded="true" visible="true" pixel-width="500" pixel-height="200" code-language="text/maple" autofit="true" wrapping="true" show-border="true" code-line-numbers="true">#Returns a list of list that has all the choords for the points around the current point withing the bounds.
pointsAround := proc(currentPos,topCorner)
	
	local cp:=currentPos;
	local aroundA := Array(1..8,1..2);
	local arrayIndex := 0;
	
	cp[1]:=cp[1]+1;#right
	if cp[1] &gt;= 0.5 and cp[1] &lt;= topCorner and cp[2] &gt;= 0.5 and cp[2] &lt;= topCorner then 
		arrayIndex:=arrayIndex+1;	
		aroundA[arrayIndex,1] := cp[1];
		aroundA[arrayIndex,2] := cp[2];
	end if;

	cp:=currentPos;
	cp[2]:=cp[2]+1;#top
	if cp[1] &gt;= 0.5 and cp[1] &lt;= topCorner and cp[2] &gt;= 0.5 and cp[2] &lt;= topCorner then 
		arrayIndex:=arrayIndex+1;	
		aroundA[arrayIndex,1] := cp[1];
		aroundA[arrayIndex,2] := cp[2];
	end if;

	cp:=currentPos;
	cp[1]:=cp[1]-1;#left
	if cp[1] &gt;= 0.5 and cp[1] &lt;= topCorner and cp[2] &gt;= 0.5 and cp[2] &lt;= topCorner then 
		arrayIndex:=arrayIndex+1;	
		aroundA[arrayIndex,1] := cp[1];
		aroundA[arrayIndex,2] := cp[2];	
	end if;

	cp:=currentPos;
	cp[2]:=cp[2]-1;#bottom
	if cp[1] &gt;= 0.5 and cp[1] &lt;= topCorner and cp[2] &gt;= 0.5 and cp[2] &lt;= topCorner then 
		arrayIndex:=arrayIndex+1;	
		aroundA[arrayIndex,1] := cp[1];
		aroundA[arrayIndex,2] := cp[2];	
	end if;

	return aroundA[1..arrayIndex]; #array of points around point
	
end proc:</EC-CodeEditor></CodeEditor-ExecGroup>
