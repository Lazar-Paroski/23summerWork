</Group><CodeEditor-ExecGroup view="presentation" labelreference="L7" drawlabel="true" applyint="true" applyrational="true" display="code"><EC-CodeEditor id="CodeEditRegion1" expanded="true" visible="true" pixel-width="500" pixel-height="200" code-language="text/maple" autofit="true" wrapping="true" show-border="true" code-line-numbers="true">#New Question Procedure

tryAnother := proc(canvas)
	# Setup
	randomize();
	local sc := Script();
	SetOption(sc, &quot;DELAY&quot;, 0);
	local x,y;
	y:= rand(1..10)();
	x:= rand(1..10)();
	# WRITE YOUR PROBLEM RANDOMIZER HERE.  This is any code that generates a new question.  
	# Then, replace the line below with one or more new variable definitions 
	
	# Displays new question	
	local txt := Text(&quot;Given %1 and %2 what is the answer.&quot;,x,y);
	
	SetActive(sc, &quot;question&quot;);
	SetText(sc, txt):
	
	# Clears existing input
	SetActive(sc, &quot;solbox&quot;);
	SetText(sc, &quot;&quot;);
	
	SetActive(sc, &quot;inputbox&quot;);
	Annotate(sc,&quot;&quot;);
	SetMath(sc, &quot;&quot;);
	
	# Updates State - this is where the variable data is stored as the user answers the question.
	SetState(sc, [x,y]);
	return ToString(sc);
end proc:</EC-CodeEditor></CodeEditor-ExecGroup>
