CheckAns := proc(canvas)
	# Setup
	local sc := Script();
	SetOption(sc, &quot;DELAY&quot;, 0);
	SetActive(sc, &quot;inputbox&quot;);
	
	# Grabs the variable data here.  Add additional variables if your question uses more than one.
	local x,y;
	local state := GetElements(canvas, &quot;state&quot;)[1]:-value;
	x,y := op(state);
	
	# Gets the solution from the getAnswer procedure.
	local sol := getAnswer(x,y);
	
	# Gets the user's input.
	local user_in := DocumentTools:-Canvas:-GetMath(cv, custom = &quot;inputbox&quot;)[1]:-math;

	# Displays correct or incorrect depending on whether the user's input equals the solution.
	# Change this if statement if you need a different method of comparing the user's input and solution.
	if user_in = sol then
		Annotate(sc, &quot;Correct.&quot;);
	else
		Annotate(sc, &quot;Incorrect.&quot;);
	end if;
	
	return ToString(sc);
end proc:</EC-CodeEditor></CodeEditor-ExecGroup>
