<Section collapsed="false" isCollapsible="true" drawButton="true" MultipleChoiceAnswerIndex="-1" MultipleChoiceRandomizeChoices="false" TrueFalseAnswerIndex="-1" EssayAnswerRows="5" EssayAnswerColumns="60"><Title><Text-field style="Heading 1" layout="Heading 1">Results</Text-field></Title><Presentation-Block><CodeEditor-ExecGroup view="presentation" inline-output="false" labelreference="L13450" drawlabel="true" applyint="true" applyrational="true" display="code"><EC-CodeEditor id="CodeEditRegion1" expanded="true" visible="true" pixel-width="500" pixel-height="200" code-language="text/maple" autofit="true" wrapping="true" show-border="true" code-line-numbers="true">results := proc(length, height1):
	local slant, volume, surface_area:
	
	local s := evalf(sqrt(height1^2+1/4*length^2)):
	local V := 1/3*height1*length^2:
	local SA := length*(length + evalf(sqrt(length^2 + 4*height1^2))):
	
	# checking if int, otherwise displaying value rounded to 2 decimal places
	if type(s, 'integer') = true then
		slant := s:
	else
		slant := MapleTA:-Builtin:-decimal(2, s):
	end if:
	
	volume := MapleTA:-Builtin:-decimal(2, V):
	
	surface_area := MapleTA:-Builtin:-decimal(2, SA):

	
	return [slant, volume, surface_area]:
end proc:</EC-CodeEditor></CodeEditor-ExecGroup></Presentation-Block><Presentation-Block>
