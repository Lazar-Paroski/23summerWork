<Section collapsed="false" isCollapsible="true" drawButton="true" MultipleChoiceAnswerIndex="-1" MultipleChoiceRandomizeChoices="false" TrueFalseAnswerIndex="-1" EssayAnswerRows="5" EssayAnswerColumns="60"><Title><Text-field style="Heading 1" layout="Heading 1">Show Explanation</Text-field></Title><Presentation-Block><CodeEditor-ExecGroup view="presentation" inline-output="false" labelreference="L13457" drawlabel="true" applyint="true" applyrational="true" display="code"><EC-CodeEditor id="CodeEditRegion5" expanded="true" visible="true" pixel-width="500" pixel-height="200" code-language="text/maple" autofit="true" wrapping="true" show-border="true" code-line-numbers="true">ShowExpl := proc(canvas)
	local state := op(GetElements(canvas, &quot;state&quot;)[1]:-value):
	local txt, expl, x:
	local sc := Script():
	SetActive(sc, &quot;explanation&quot;);
	ClearGroup(sc):
	if state = &quot;radius&quot; then
		expl := [Text(&quot;Volume: %1.&quot;,V=Pi*r^2*h),
				Text(&quot;Suppose %1 = %2.&quot;, r__new, 2*r),
				Text(&quot;Then, %1 = %2 = %3.&quot;, V__new, Pi*(r__new)^2*h, Pi*(2*r)%^2*h),
				Text(&quot;Therefore, %1 = %2 = %3, the new volume is four times as large.&quot;,V__new,4%*Pi*r^2*h,4*V),
				Text(&quot;See %1 for more details.&quot;, Link(&quot;https://learn.maplesoft.com/doc/volume-of-cylinders-and-cones-igqw0ncanq&quot;, &quot;Volume of Cylinders and Cones&quot;))]:
		SetText(sc, &quot;Explanation&quot;, bold = true, custom = &quot;explanation&quot;):
		for x from 1 to numelems(expl) do
			SetText(sc, expl[x], 'where'=&quot;below&quot;);
		end do;
	elif state = &quot;height1&quot; then
		expl := [Text(&quot;Volume: %1.&quot;,V=Pi*r^2*h),
				Text(&quot;Suppose %1 = %2.&quot;, h__new, 2*h),
				Text(&quot;Then, %1 = %2 = %3.&quot;, V__new, Pi*r^2*h__new, 2*Pi*r^2*h),
				Text(&quot;Therefore, %1 = %2 , the new volume is two times as large.&quot;,V__new,2*V),
				Text(&quot;See %1 for more details.&quot;, Link(&quot;https://learn.maplesoft.com/doc/volume-of-cylinders-and-cones-igqw0ncanq&quot;, &quot;Volume of Cylinders and Cones&quot;))]:
		SetText(sc, &quot;Explanation&quot;, bold = true, custom = &quot;explanation&quot;):
		for x from 1 to numelems(expl) do
			SetText(sc, expl[x], 'where'=&quot;below&quot;);
		end do;
	elif state = &quot;both&quot; then
		expl := [Text(&quot;Surface Area: %1.&quot;,S=2*Pi*r*(r+h)),
				Text(&quot;Suppose %1 = %2 and %3 = %4.&quot;, h__new, 2*h, r__new, 2*r),
				Text(&quot;Then, %1 = %2 = %3.&quot;, S__new, 2%*Pi*r__new*(r__new+h__new), 2%*Pi%*2%*r*(2*r+2*h)),
				Text(&quot;Therefore, %1 = %2 = %3, the new surface area is four times as large.&quot;,S__new,4%*2%*Pi*r*(r+h),4*S),
				Text(&quot;See %1 for more details.&quot;, Link(&quot;https://learn.maplesoft.com/doc/surface-area-of-cylinders-krs5uju7f6/surface-area-of-cylinders&quot;, &quot;Surface Area of Cylinders&quot;))]:
		SetText(sc, &quot;Explanation&quot;, bold = true, custom = &quot;explanation&quot;):
		for x from 1 to numelems(expl) do
			SetText(sc, expl[x], 'where'=&quot;below&quot;);
		end do;
	else
		txt := Text(&quot;Press 'Double Radius', 'Double height1', or \134n'Double Both'; then press 'Explanation' for a \134ndetailed explanation of the proportionalities.&quot;):
		SetActive(sc, &quot;explanation&quot;):
		ClearGroup(sc):
		SetText(sc, txt):
	end if:
	ToString(sc):
end proc:</EC-CodeEditor></CodeEditor-ExecGroup></Presentation-Block><Presentation-Block>
