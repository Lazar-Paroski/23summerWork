<Section collapsed="false" isCollapsible="true" drawButton="true" MultipleChoiceAnswerIndex="-1" MultipleChoiceRandomizeChoices="false" TrueFalseAnswerIndex="-1" EssayAnswerRows="5" EssayAnswerColumns="60"><Title><Text-field style="Heading 1" layout="Heading 1">Plot</Text-field></Title><Presentation-Block><CodeEditor-ExecGroup view="presentation" inline-output="false" labelreference="L13464" drawlabel="true" applyint="true" applyrational="true" display="code"><EC-CodeEditor id="CodeEditRegion2" expanded="true" visible="true" pixel-width="500" pixel-height="200" code-language="text/maple" autofit="true" wrapping="true" show-border="true" code-line-numbers="true">plt := proc(length, height1):
	local sls := 1/5:
	local h := convert(height1, string):
	local l := convert(length, string):
	local s := convert (results(length, height1)[1], string):


	#setting inital plot values
	#Pyramid plots
	local polyBase := polygon([[length,length, 0], [-1*length, length, 0], [-1*length, -1*length, 0], [length, -1*length, 0]]);
	local p1 := pyramid(polyBase,'height'=height1,transparency = 0.5,color = &quot;LightCoral&quot;);
	#line plots
	local lineV := curve([[length+1,0,0],[length+1,0,height1]],thickness=10,color=black);
	local lineHd := curve([[length+1,0,height1],[0,0,height1]],thickness = 10,color=black,linestyle = dash);
