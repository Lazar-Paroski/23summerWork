<Section collapsed="false" isCollapsible="true" drawButton="true" MultipleChoiceAnswerIndex="-1" MultipleChoiceRandomizeChoices="false" TrueFalseAnswerIndex="-1" EssayAnswerRows="5" EssayAnswerColumns="60"><Title><Text-field style="Heading 1" layout="Heading 1">Generate Canvas</Text-field></Title><Presentation-Block><CodeEditor-ExecGroup view="presentation" inline-output="false" labelreference="L13453" drawlabel="true" applyint="true" applyrational="true" display="code"><EC-CodeEditor id="CodeEditRegion9" expanded="true" visible="true" pixel-width="500" pixel-height="200" code-language="text/maple" autofit="true" wrapping="true" show-border="true" code-line-numbers="true">local radius := 3.1:
local height1 := 5.4:
local prop := &quot;null&quot;:
local res := results(radius, height1):

#setting inital plot values
#Pyramid plots
polyBase := polygon([[3.1, 3.1, 0], [-3.1, 3.1, 0], [-3.1, -3.1, 0], [3.1, -3.1, 0]]):
p1 := pyramid(polyBase,'height'=5.4,transparency = 0.5,color = &quot;LightCoral&quot;):
#line plots
lineV := curve([[4.1,0,0],[4.1,0,5.4]],thickness=10,color=black):
lineHd := curve([[4.1,0,5.4],[0,0,5.4]],thickness = 10,color=black,linestyle = dash):
lineH := curve([[3.1,4.1,0],[-3.1,4.1,0]],thickness = 10,color=black):
#Text Plots
vT:= textplot3d([5, 0, 3, &quot;5.4&quot;],'font' = [&quot;times&quot;, &quot;roman&quot;, 15]):
hT:= textplot3d([0, 4.1, -0.8, &quot;3.1&quot;],'font' = [&quot;times&quot;, &quot;roman&quot;, 15]):


#Making Canvas
 cv := NewCanvas(
		[Text(&quot;Volume and Surface Area of a Square Pyramid&quot;, bold = true, fontsize = 24, position = titlePos),
		 Text(&quot;Enter new values for radius and height and press 'Update' to see what effect this has\134non the volume and surface area of a square pyramid. Alternatively, try the preset \134n'Double Length','Double height', or 'Double Both' buttons.&quot;, position = promptPos),
           Group([
           Text(&quot;Square Pyramid&quot;, bold = true),
           
           Plot3D(display([p1,lineV,lineHd,lineH,vT,hT],axes=none,scaling=constrained))
           
           ], position=plotHerePos),
           Group([Text(&quot;Length&quot;, bold = true), 
           	Math(radius, custom = &quot;length&quot;), 
           	Text(&quot;height&quot;, bold = true), 
           	Math(height1, custom = &quot;height1&quot;)], 
           		position = lengthheight1Pos),
           ScriptButton(&quot;Update&quot;, update, include=[plt,results], 
           	position = updatePos, 
           	size = buttonSize),
           ScriptButton(&quot;Double Length&quot;, double_length, include=[plt,results], 
           	position = doubleRadPos, 
           	size = buttonSize),
           ScriptButton(&quot;Double height&quot;, double_height1, include=[plt,results],
           	position = doubleheight1Pos, 
           	size = buttonSize),
           ScriptButton(&quot;Double Both&quot;, double_both, include=[plt,results],
           	position = doubleBothPos, 
           	size = buttonSize),
           ScriptButton(&quot;Explanation&quot;, ShowExpl, include=[plt,results], 
           	position = explainPos, 
           	size = buttonSize),
           Group([
           	Text(&quot;Slant height&quot;, bold = true),
           	Math(res[1], custom = &quot;slant&quot;, result = false),
           	Text(&quot;Volume&quot;, bold = true), 
           	Math(res[2], custom = &quot;volume&quot;, result = false), 
           	Text(&quot;Surface Area&quot;, bold = true), 
           	Math(res[3], custom = &quot;surface_area&quot;, result = false)],
           		position = volSurfAreaPos),
           Group([Text(&quot;Proportionality&quot;, bold = true), 
           	Text(&quot;&quot;, custom = &quot;question&quot;)],
           		position = proportionPos),
           Group([Text(&quot;Explanation&quot;, bold = true), 
           	Text(&quot;&quot;)], 
           		custom = &quot;explanation&quot;,
           		position = explainHerePos),
           State([prop])
              ], showpanel = false):
           

ShareCanvas(cv):</EC-CodeEditor></CodeEditor-ExecGroup>
